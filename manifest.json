{
  "manifest_version": 3,
  "name": "Always Early",
  "version": "1.0.1",
  "description": "Auto-opens browser tabs for your upcoming Google Calendar meetings before they start.",
  "homepage_url": "https://cbmca.github.io/meeting-concierge/",
  "permissions": [
    "identity",
    "storage",
    "alarms"
  ],
  "oauth2": {
    "client_id": "17127521381-em52cbujakjrsnd3l6g7ambffrpscrlj.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Always Early"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
